{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "globals": {
    "$": true,
    "jQuery": true
  },
  "plugins": ["prettier", "jest"],
  "extends": ["eslint:recommended", "plugin:jest/recommended", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off"
  }
}
