{
  "name": "boobreaks",
  "version": "4.0.1",
  "description": "Use Bootstraps breakpoints inside your JavaScript!",
  "keywords": [
    "bootstrap",
    "breakpoints",
    "utility"
  ],
  "homepage": "https://github.com/muuvmuuv/boobreaks",
  "bugs": "https://github.com/muuvmuuv/boobreaks/issues",
  "repository": "git@github.com:muuvmuuv/boobreaks.git",
  "license": "MIT",
  "author": {
    "name": "Marvin Heilemann",
    "email": "marvin.heilemann+github@googlemail.com",
    "url": "https://marvin.digital/"
  },
  "source": "src/index.ts",
  "main": "dist/boobreaks.js",
  "umd:main": "dist/boobreaks.umd.js",
  "module": "dist/boobreaks.mjs",
  "types": "dist/boobreaks.d.ts",
  "unpkg": "dist/boobreaks.umd.js",
  "scripts": {
    "start": "npm run watch",
    "build": "microbundle --name Boobreaks",
    "watch": "microbundle watch --name Boobreaks",
    "build-docs": "microbundle -f umd -o docs --name Boobreaks",
    "watch-docs": "microbundle watch -f umd -o docs --name Boobreaks",
    "prepublishOnly": "npm run build",
    "serve": "serve docs"
  },
  "dependencies": {
    "bootstrap": "^4.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "microbundle": "^0.11.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "serve": "^11.2.0"
  }
}